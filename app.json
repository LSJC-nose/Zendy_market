{
  "expo": {
    "name": "ZendyMarket",
    "slug": "zendymarket",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_zendy.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_zendy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zendymarket.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_zendy.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "notification": {
          "channelId": "default"
        }
      },
      "package": "com.zendymarket.app"
    },
    "web": {
      "favicon": "./assets/logo_zendy.png"
    }
  }
}
