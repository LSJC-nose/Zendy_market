{
  "expo": {
    "jsEngine": "hermes",
    "name": "ZendyMarket",
    "slug": "ZendyMarket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_zendy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-notifications"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_zendy.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "notification": {
          "channelId": "default"
        }
      }
    },
    "web": {
      "favicon": "./assets/logo_zendy.png"
    },
    "extra": {
      "cli": {
        "appVersionSource": "manifest"
      }
      
    },
    "owner": "santis"
  }
}
